<h1>Listing personals</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Photo</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @personals.each do |personal| %>
      <tr>
        <td><%= personal.first_name %></td>
        <td><%= personal.last_name %></td>
        <td><%= personal.photo %></td>
        <td><%= link_to 'Show', [:admin, personal] %></td>
        <td><%= link_to 'Edit', edit_admin_personal_path(personal) %></td>
        <td><%= link_to 'Destroy', [:admin, personal], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Personal', new_admin_personal_path %>
